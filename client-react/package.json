  "scripts": {
    "start": "craco start",
    "build": "craco --max_old_space_size=4096 build",
    "test": "craco test --env=jsdom --transformIgnorePatterns \"node_modules/(?!(office-ui-fabric-react|lodash-es)/)\"",
    "test:ci": "craco test --env=jsdom --testResultsProcessor ./node_modules/jest-junit --coverage",
    "test:debug": "craco --inspect-brk test --no-cache --verbose=false --runInBand",
    "storybook": "start-storybook -p 9009 -s public",
    "analyze": "craco build --stats && webpack-bundle-analyzer build/bundle-stats.json -m static -r build/bundle-stats.html -O",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-links": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "@types/applicationinsights-js": "^1.0.9",
    "@types/enzyme": "^3.1.17",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/i18next-xhr-backend": "^1.4.1",
    "@types/jest": "^24.0.0",
    "@types/joi": "^14.3.2",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^10.12.24",
    "@types/reach__router": "^1.2.3",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/react-i18next": "^8.1.0",
    "@types/webpack-env": "^1.13.7",
    "@types/yup": "^0.26.9",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "axios-mock-adapter": "^1.16.0",
    "babel-core": "^7.0.0-bridge.0",
    "core-js": "^3.15.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest-date-mock": "^1.0.7",
    "jest-junit": "^6.2.1",
    "jest-mock-axios": "^2.1.11",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "react-test-renderer": "^16.8.1",
    "typescript": "^4.3.5",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "proxy": "https://localhost:44300",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts}",
      "!src/**/*.stories.ts",
      "!src/**/*.d.ts",
      "!src/**/*.styles.ts",
      "!src/**/*.types.ts",
      "!src/theme/**",
      "!<rootDir>/node_modules/",
      "!src/mocks/**",
      "!src/*",
      "!src/models/**",
      "!src/modules/services.ts",
      "!src/utils/Guid.ts",
      "!src/utils/scenario-checker/**"
    ],
    "coverageReporters": [
      "text",
      "cobertura",
      "html"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "engines": {
    "yarn": "NO LONGER USED - Please use npm"
  }
