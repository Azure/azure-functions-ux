{
  "name": "azure-functions-ux-server",
  "version": "0.0.0",
  "scripts": {
    "watch": "concurrently npm:watch:*",
    "watch:ts": "nodemon -e ts -w ./src -x npm run serve:dev",
    "watch:resources": "nodemon -e resx -w ./Resources -x npm run build:resources",
    "serve:dev": "ts-node --inspect src/server.ts",
    "build:resources": "gulp build-all",
    "build": "gulp build-production && tsc",
    "pack-cert-mac": "openssl pkcs12 -inkey selfcertkey.pem  -in selfcert.pem -export -out certinstall.pfx"
  },
  "workspaces": {
    "nohoist": [
      "**"
    ]
  },
  "dependencies": {
    "applicationinsights": "^1.0.7",
    "async": "^2.6.1",
    "async-file": "^2.0.2",
    "axios": "^0.18.0",
    "azure-storage": "^2.10.2",
    "body-parser": "^1.18.3",
    "circular-json": "^0.5.9",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "dotenv": "^4.0.0",
    "express": "^4.16.4",
    "express-rate-limit": "^3.3.2",
    "express-react-views": "^0.11.0",
    "express-session": "^1.15.6",
    "hsts": "^2.1.0",
    "jsonwebtoken": "^8.4.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/async": "^2.0.50",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.17.0",
    "@types/circular-json": "^0.4.0",
    "@types/compression": "^0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/cookie-session": "^2.0.36",
    "@types/express": "^4.16.0",
    "@types/express-rate-limit": "^2.9.3",
    "@types/express-session": "^1.15.11",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.7",
    "@types/react": "^16.7.13",
    "@types/uuid": "^3.4.4",
    "concurrently": "^4.1.0",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-decompress": "^2.0.2",
    "gulp-download": "^0.0.1",
    "gulp-json-editor": "^2.4.4",
    "gulp-merge-json": "^1.3.1",
    "gulp-rename": "^1.4.0",
    "gulp-string-replace": "^1.1.2",
    "gulp-token-replace": "^1.1.2",
    "merge-stream": "^1.0.1",
    "nodemon": "^1.11.0",
    "prettier": "^1.15.3",
    "resx": "^1.2.0",
    "through2": "^3.0.0",
    "ts-node": "^3.1.0",
    "typescript": "^3.2.2"
  }
}
