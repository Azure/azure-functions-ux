{
  "name": "server",
  "version": "0.0.0",
  "description": "Backend for Functions Portal",
  "author": "Microsoft",
  "license": "MIT",
  "private": true,
  "scripts": {
    "bundle": "gulp build-production",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/server.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "build:resources": "gulp build-all"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.2",
    "@nestjs/core": "^6.0.2",
    "@nestjs/platform-express": "^6.0.2",
    "@nestjs/swagger": "^3.0.2",
    "applicationinsights": "^1.2.0",
    "async-file": "^2.0.2",
    "axios": "^0.18.0",
    "azure-storage": "^2.10.2",
    "cache-manager": "^2.9.0",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "csurf": "^1.9.0",
    "dotenv": "^7.0.0",
    "express-rate-limit": "^3.4.0",
    "helmet": "^3.16.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.1",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.4.0",
    "swagger-ui-express": "^4.0.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.2",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.6",
    "@types/supertest": "^2.0.7",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "del": "^4.0.0",
    "gulp": "^4.0.0",
    "gulp-decompress": "^2.0.2",
    "gulp-download-stream": "^0.0.19",
    "gulp-json-editor": "^2.5.1",
    "gulp-merge-json": "^1.3.1",
    "gulp-rename": "^1.4.0",
    "gulp-string-replace": "^1.1.2",
    "gulp-token-replace": "^1.1.5",
    "jest": "^24.5.0",
    "merge-stream": "^1.0.1",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "gulp-prettier": "^2.1.0",
    "resx": "^1.2.0",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "through2": "^3.0.1",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.8.0",
    "tslint": "5.14.0",
    "typescript": "^3.3.4000",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
