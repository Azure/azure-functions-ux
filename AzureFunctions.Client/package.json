    "es6-shim": "^0.33.3",
    "font-awesome": "^4.5.0",
    "ng2-nvd3": "github:ahmelsayed/ng2-nvd3",
    "nvd3": "^1.8.2",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.24",
    "zone.js": "0.5.15"
